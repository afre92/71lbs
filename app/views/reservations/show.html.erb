<div class="content" style="height: 100%; padding-top: 80px; padding-bottom: 80px;">
<div class="content">
<p id="notice"><%= notice %></p>

<p>
  <strong>From:</strong>
  <%= @reservation.from %>
</p>

<p>
  <strong>To:</strong>
  <%= @reservation.to %>
</p>

<p>
  <strong>User:</strong>
  <%= @reservation.user %>
</p>

<%= link_to 'Edit', edit_reservation_path(@reservation) %> |
<%= link_to 'Back', reservations_path %>


<div class="field">
  <h2> rooms </h2>

  <%= render "rooms/form" %>

  <ul>
    <% @reservation.rooms.each do |room| %>
    <li>

       <%= room.number %>
    </li>
    <% end %>


</div>
</div>
</div>
